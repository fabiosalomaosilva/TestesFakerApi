{
    "version": 2,
    "builds": [
        {
            "src": "src/api.js",
            "use": "@now/node"
        }
    ],
    "routes": [
        {
            "src": "/swagger(.*)",
            "dest": "/swagger"
        },
        {
            "src": "/(.*)",
            "dest": "src/api.js"
        }
    ]
}