{
    "version": 2,
    "builds": [
        {
            "src": "src/api.js",
            "use": "@now/node"
        }
    ],
    "routes": [
        {
            "src": "/swagger(.*)",
            "dest": "/api/swagger"
        },
        {
            "src": "/api/swagger/swagger(.*)",
            "dest": "/api/swagger"
        },
        {
            "src": "/(.*)",
            "dest": "src/api.js"
        }
    ]
}